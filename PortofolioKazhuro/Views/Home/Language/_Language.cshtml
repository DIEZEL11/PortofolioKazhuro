@using Microsoft.AspNetCore.Mvc.Rendering
@using PortofolioKazhuro.Models.Language
@model IEnumerable<LanguageSkill>
@if (Model.Count() > 0)
{
    <div class="card mb-4 shadow-sm border-0 rounded-4 w-100">
        <div class="card-header bg-gradient bg-primary text-white rounded-top-4 py-4 px-4 d-flex align-items-center">


            <i class="fas fa-code me-2"></i>
                <h5 class="mb-0 fw-bold">Языки</h5>
         
        </div>

        <div class="card-body">
            @await Html.PartialAsync("Language/_LanguageList", Model)
        </div>
    </div>
}


